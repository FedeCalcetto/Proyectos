@using Dominio;
@model List<Usuario>
@{
    ViewData["Title"] = "Listas";
}

<h1>Lista de Usuarios ordenados</h1>


<table class="table">
    <thead>
        <tr>
            <th>Nombre</th>
            <th>Apellido</th>
        </tr>
    </thead>
    <tbody>
        @foreach (Usuario usuario in Model)
        {
            if (usuario is Miembro miembro)
            {
                <tr>
                    <td>@miembro.Nombre</td>
                    <td>@miembro.Apellido</td>
                </tr>
            }
        }
    </tbody>
</table>
